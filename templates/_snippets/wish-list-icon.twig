{% set list = craft.wishlist.lists().default(true).one() %}

{% if list|length %}
	<a href="/wish-list" class="wish-list-link text-rug-yellow">
		<i class="fas fa-clipboard-list"></i>
	</a>
	{% else %}
	<a href="/wish-list" class="wish-list-link text-rug-light-blue">
		<i class="fas fa-clipboard-list"></i>
	</a>
{% endif %}
