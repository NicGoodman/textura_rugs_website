<article id="" class="">
<h2>{{ name }}</h2>
	<picture>
		<img src="{{ thumbnail.src() }}" data-srcset="{{ thumbnail.srcset() }}" alt="{{ name }}">
	</picture>
     <form method="POST">
        <input type="hidden" name="action" value="wishlist/items/toggle">
        {{ csrfInput() }}

        <input type="hidden" name="elementId" value="{{ entry.id }}">

        <input type="submit" value="{% if entry.title in list.items %}Remove From{% else %}Add To{% endif %} List" class="">
    </form>
</article>