<section id="inventory-feed" class="flex flex-row flex-wrap items-start justify-center w-full">
	{% set list = craft.wishlist.lists().default(true).one() %}
	{% for entry in inventoryQuery.all() %}
		{% set name = entry.productTitle %}
		{% if entry.primaryImage|length %}
			{% set thumbnail = entry.primaryImage.one().optimizedImages %}
		{% endif %}
		{% include "_content/rug-block" %}
	{% endfor %}
</section>
<div class="p-4 border-t mixitup-page-list font-bioRhyme text-rug-dark-blue border-dark-blue"></div>
