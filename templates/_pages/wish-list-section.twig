<div id="wish-list" class="wishListContent" x-show="show">
{# Get the list #}
{% set list = craft.wishlist.lists().default(true).one() %}
{% if list|length %}
{# Display list items #}
<ul>
    {% for item in list.items.all() %}
        <li>{{ item.title }}</li>
    {% endfor %}
</ul>
{% endif %}
</div>